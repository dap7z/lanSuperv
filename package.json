{
  "name": "lan-superv",
  "version": "0.4.000",
  "description": "Node.js app for lan supervision",
  "author": "Damien Pointier",
  "license": "MIT",
  "bin": "application.js",
  "pkg": {
    "assets": [
      "web/**/*",
      "plugins/local-responses/**/*",
      "plugins/remote-requests/**/*",
      "node_modules/raw-socket/**/*"
    ],
    "scripts": [],
    "targets": [
      "node10-win-x64"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "auto-updater": "^1.0.2",
    "body-parser": "^1.20.2",
    "child-process-promise": "^2.2.1",
    "child_process": "^1.0.2",
    "cidr-range": "^2.0.0",
    "cross-spawn": "^7.0.3",
    "express": "^4.18.2",
    "ext-ip": "^0.3.8",
    "fs": "0.0.2",
    "gun": "^0.2020.1241",
    "is-port-available": "^0.1.5",
    "json-formatter-js": "^2.3.4",
    "lan-discovery": "^1.0.2",
    "netmask": "^2.0.2",
    "node-machine-id": "^1.1.12",
    "ping-bluebird": "0.0.3",
    "power-off": "^1.1.2",
    "promise": "^8.3.0",
    "sleep-mode": "^1.1.0",
    "then-request": "^6.0.2",
    "vue": "^2.7.14",
    "vue-gun": "0.0.3",
    "wol": "^1.0.7",
    "yargs": "^17.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dap7z/lanSuperv"
  },
  "build": {
    "appId": "com.github.dap7z.lanSuperv",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    },
    "publish": [
      {
        "provider": "github",
        "owner": "dap7z",
        "repo": "lanSuperv"
      }
    ]
  },
  "auto-updater": {
    "repo": "dap7z/lanSuperv",
    "branch": "master"
  },
  "main": "module.js",
  "scripts": {
    "start": "node start.js",
    "dev": "webpack --mode development",
    "build": "webpack --mode production && pkg ."
  }
}
