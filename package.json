{
  "name": "lan-superv",
  "version": "0.3.445",
  "description": "Node.js app for lan supervision",
  "author": "Damien Pointier",
  "license": "MIT",
  "engines": {
    "node": "8.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "electron": "^1.4.15",
    "electron-builder": "^20.19.2",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "auto-updater": "^1.0.2",
    "body-parser": "^1.18.1",
    "child_process": "^1.0.2",
    "default-network": "^0.2.0",
    "electron-log": "^1.3.0",
    "express": "^4.15.4",
    "ext-ip": "^0.3.8",
    "fs": "0.0.1-security",
    "gun": "^0.9.991",
    "http": "0.0.0",
    "is-port-available": "^0.1.4",
    "json-formatter-js": "^2.2.0",
    "my-local-netmask": "^1.0.0",
    "netmask": "^1.0.6",
    "node-machine-id": "^1.1.9",
    "node-nmap": "^4.0.0",
    "ping-bluebird": "0.0.3",
    "power-off": "^1.1.2",
    "promise": "^8.0.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "sleep-mode": "^1.1.0",
    "then-request": "^4.1.0",
    "wol": "^1.0.3",
    "yargs": "^12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dap7z/lanSuperv"
  },
  "build": {
    "appId": "com.github.dap7z.lanSuperv",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    },
    "publish": [
      {
        "provider": "github",
        "owner": "dap7z",
        "repo": "lanSuperv"
      }
    ]
  },
  "auto-updater": {
    "repo": "dap7z/lanSuperv",
    "branch": "master"
  },
  "main": "module.js",
  "scripts": {
    "start": "node start.js",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "publish": "build --win -p always"
  }
}
