{
  "name": "lan-superv",
  "version": "0.3.800",
  "description": "Node.js app for lan supervision",
  "author": "Damien Pointier",
  "license": "MIT",
  "bin": "application.js",
  "pkg": {
    "assets": [
      "web/**/*",
      "plugins/local-responses/**/*",
      "plugins/remote-requests/**/*"
    ],
    "scripts": [],
    "targets": [
      "node10-win-x64"
    ]
  },
  "engines": {
    "node": "8.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "auto-launch": "^5.0.5",
    "auto-updater": "^1.0.2",
    "body-parser": "^1.19.0",
    "child-process-promise": "^2.2.1",
    "child_process": "^1.0.2",
    "cidr-range": "^2.0.0",
    "cross-spawn": "^6.0.5",
    "express": "^4.17.1",
    "ext-ip": "^0.3.8",
    "fs": "0.0.2",
    "gun": "^0.2019.515",
    "http": "0.0.0",
    "is-port-available": "^0.1.5",
    "json-formatter-js": "^2.2.1",
    "lan-discovery": "^1.0.0",
    "netmask": "^1.0.6",
    "node-machine-id": "^1.1.12",
    "ping-bluebird": "0.0.3",
    "power-off": "^1.1.2",
    "promise": "^8.0.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sleep-mode": "^1.1.0",
    "then-request": "^6.0.2",
    "vue": "^2.6.10",
    "vue-gun": "0.0.3",
    "wol": "^1.0.6",
    "yargs": "^13.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dap7z/lanSuperv"
  },
  "build": {
    "appId": "com.github.dap7z.lanSuperv",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    },
    "publish": [
      {
        "provider": "github",
        "owner": "dap7z",
        "repo": "lanSuperv"
      }
    ]
  },
  "auto-updater": {
    "repo": "dap7z/lanSuperv",
    "branch": "master"
  },
  "main": "module.js",
  "scripts": {
    "start": "node start.js",
    "dev": "webpack --mode development",
    "build": "webpack --mode production && pkg ."
  }
}
